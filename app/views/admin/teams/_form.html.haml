= form_for @admin_team do |f|
  - if @admin_team.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_team.errors.count, "error")} prohibited this team from being saved:"
      %ul
        - @admin_team.errors.full_messages.each do |msg|
          %li= msg

  .actions
    = f.submit 'Save'
